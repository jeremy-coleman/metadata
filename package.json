{
  "name": "@proteria/metadata",
  "version": "0.3.2",
  "description": "Babel plugin to emit decorator metadata like typescript compiler",
  "main": "index.js",
  "module": "index.mjs",
  "repository": "https://github.com/proteriax/metadata.git",
  "author": "Alex <github@aet.ac>",
  "license": "MIT",
  "keywords": [
    "babel",
    "babel-plugin",
    "babel-typescript",
    "decorators",
    "reflect-metadata"
  ],
  "scripts": {
    "docs": "typedoc src/index.ts",
    "build": "rollup -c && ./scripts/build-package.json.js < package.json > lib/package.json",
    "check-types": "tsc --noEmit",
    "release": "yarn test && yarn build && release-it",
    "test": "jest",
    "test:dev": "jest --watch",
    "test:ci": "jest --ci --runInBand --coverage",
    "postinstall": "patch-package"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.13.16",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.15",
    "@babel/plugin-syntax-class-properties": "^7.12.13",
    "@babel/plugin-transform-modules-commonjs": "^7.13.8",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/template": "^7.12.13",
    "@babel/types": "^7.13.17",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@types/dedent": "^0.7.0",
    "@types/jest": "^26.0.22",
    "@types/prettier": "^2.2.3",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/parser": "4.22.0",
    "babel-test": "^0.2.4",
    "cli-highlight": "^2.1.11",
    "conventional-changelog-cli": "^2.1.1",
    "dedent": "^0.7.0",
    "eslint": "7.25.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-react": "7.23.2",
    "husky": "4.2.3",
    "jest": "^26.6.3",
    "jest-file-snapshot": "^0.5.0",
    "patch-package": "^6.4.7",
    "prettier": "^2.2.1",
    "reflect-metadata": "^0.1.13",
    "regexp.escape": "^1.1.0",
    "release-it": "14.6.1",
    "rollup": "^2.45.2",
    "rollup-plugin-ts": "^1.3.12",
    "sass": "^1.32.11",
    "typedoc": "^0.20.36",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "tslib": "^2.2.0"
  },
  "upstream": {
    "repository": "git@github.com:leonardfactory/babel-plugin-transform-typescript-metadata.git",
    "commit": "ce4710a"
  },
  "prettier": {
    "arrowParens": "avoid",
    "trailingCommas": "all",
    "singleQuote": false
  },
  "eslintIgnore": [
    "test/**/*.js",
    "lib"
  ],
  "peerDependencies": {
    "reflect-metadata": "*"
  }
}
